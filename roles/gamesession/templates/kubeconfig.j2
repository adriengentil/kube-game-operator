apiVersion: v1
clusters:
- cluster:
    certificate-authority-data: GET-THIS-INFO
    server: GET-THIS-INFO
  name: kube-game-cluster
contexts:
- context:
    cluster: kube-game-cluster
    namespace: lobby
    user: kube-game-player
  name: kube-game-player-context
current-context: kube-game-player-context
kind: Config
preferences: {}
users:
- name: kube-game-player
  user:
    client-certificate-data: {{ player_crt.stdout }}
    client-key-data: {{ player_key.stdout | b64encode }}
